    "import numpy as np \n",
    "import matplotlib.pyplot as plt \n",
    "\n",
    "def potensial(r,Q,R):\n",
    "    V = 0.\n",
    "    for i in range(len(R)):\n",
    "        Ri = r - R[i]\n",
    "        Qi = Q[i]\n",
    "        Rinorm = np.linalg.norm(Ri)\n",
    "        V = V + Qi/Rinorm\n",
    "    return V\n",
    "\n"
   ]
